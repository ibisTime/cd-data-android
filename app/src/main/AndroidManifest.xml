<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chengdai.cddata">

    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />

    <!--用于获取运营商信息，用于支持提供运营商信息相关的接口-->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"></uses-permission>
    <!--用于访问网络，网络定位需要上网-->
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <!--用于读取手机当前的状态-->
    <uses-permission android:name="android.permission.READ_PHONE_STATE"></uses-permission>
    <!--用于写入缓存数据到扩展存储卡-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>


    <application
        android:name=".base.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".models.common.activitys.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--二要素实名认证-->
        <activity android:name=".models.certification.activitys.CardAndNameInfoCheckActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan"
            />
        <!--四要素实名认证-->
        <activity android:name=".models.certification.activitys.FourInfoCheckActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan"
            />
        <!--芝麻分数获取-->
        <activity android:name=".models.certification.activitys.ZMNumGetActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan"
            />
        <!--芝麻分数获取 第二版-->
        <activity android:name=".models.certification.activitys.ZMNumGet2Activity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan"
            />
        <!--行业关注名单-->
        <activity android:name=".models.certification.activitys.IndustryFocusOnActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan"
            />


        <!--欺诈评分验证-->
        <activity android:name=".models.certification.activitys.FraudNumCheckActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan"
            />

        <!--申请欺诈评分-->
        <activity android:name=".models.certification.activitys.GetFraudNumActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan"
            />

        <!--欺诈关注清单-->
        <activity android:name=".models.certification.activitys.FraudLookListActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan"
            />
        <!--认证结果 二要素-->
        <activity android:name=".models.certesults.CertiResultsByNameAndIdCardActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan"
            />
        <!--认证结果 四要素-->
        <activity android:name=".models.certesults.CertiResultsByFourActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan"
            />
  <!--认证结果 申请欺诈评分-->
        <activity android:name=".models.certesults.CertiResultsByQizhaPingFenActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan"
            />

        <!--认证结果 详细信息-->
        <activity android:name=".models.certesults.CertiResultsByDetailsInfoActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan"
            />

        <!--认证结果 详细信息-->
        <activity android:name=".models.certesults.CertiResultsByDetailsInfo2Activity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan"
            />

        <!--芝麻认证-->
        <activity
            android:name=".models.certification.activitys.ZMCertificationActivity"
            android:launchMode="singleTask">
            <!-- 需要添加下面的intent-filter配置 -->
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                     <!--cddata://certi.back-->
                <data
                    android:host="certi.back"
                    android:scheme="cddata" />
            </intent-filter>
        </activity>


<!--芝麻信用-->
        <activity
            android:name="com.android.moblie.zmxy.antgroup.creditsdk.app.SDKActivity"
            android:label="芝麻信用"
            android:screenOrientation="portrait"
            />

        <activity android:name="com.megvii.zhimasdk.MGLivenessDetectActivity" />
        <activity android:name="com.megvii.zhimasdk.MGLoadActivity" />
        <activity android:name="com.megvii.zhimasdk.MGWebViewActivity" />
        <activity
            android:name="com.moxie.client.MainActivity">
        </activity>
    </application>

</manifest>